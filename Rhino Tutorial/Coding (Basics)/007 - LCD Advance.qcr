int main(void)
{
    char buffer[7];
    int  num,i,j;

	INIT();
	LCD_INIT(LCD_DISP_ON);
	num=0;
	
	for (i=1;i<=99;i++)
	{
		for (j=1;j<=10;j++)
		{
			itoa( i , buffer, 10);
	        	LCD_PRINT(buffer);
			LCD_PRINT(" X ");
			itoa( j , buffer, 10);
	        	LCD_PRINT(buffer);
			LCD_PRINT(" = ");
			itoa(i * j , buffer, 10);
	        	LCD_PRINT(buffer);
			TOGGLELED1();
			DELAYMS(500);	
			LCD_PRINT("\n");
		}
		TOGGLELED2();
		DELAYS(1);
		LCD_CLRSCR();
	}
}
